  },
  "keywords": [
    "slide",
    "dsl",
    "context",
    "presentation"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  }
}
