{
  "name": "@slidejs/runner-revealjs",
  "version": "0.1.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@slidejs/context": "workspace:*",
    "@slidejs/core": "workspace:*",
    "@slidejs/dsl": "workspace:*",
    "@slidejs/runner": "workspace:*",
    "reveal.js": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/reveal.js": "^5.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
